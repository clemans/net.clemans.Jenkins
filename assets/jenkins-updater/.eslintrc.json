{
  "env": {
    "node": true,
    "es6": true,
    "jest": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly",
    "console": "readonly",
    "describe": "readonly",
    "it": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "requireConfig": false,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "no-prototype-builtins": "off",
    "no-undef": "warn",
    "no-unused-vars": "warn",
    "no-useless-escape": "warn"
  }
}