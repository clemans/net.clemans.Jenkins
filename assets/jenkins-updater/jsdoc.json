{
  "source": {
    "include": [
      "bin",
      "src"
    ],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "recurse": true,
    "destination": "./docs/",
    "readme": "./docs/README.md",
    "template": "./node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "theme": "dark",
      "title": "Jenkins Updater"
    }
  }
}